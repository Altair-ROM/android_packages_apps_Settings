<PreferenceScreen
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:settings="http://schemas.android.com/apk/res/com.android.settings"
    android:title="@string/settings_label"
    android:key="parent">

    <!-- WIRELESS and NETWORKS -->
    <PreferenceCategory
            android:title="@string/header_category_wireless_networks" />

        <!-- Wi-Fi settings -->
        <com.android.settings.IconPreferenceScreen
                android:key="wifi_settings"
                android:title="@string/settings_title_wifi"
                settings:icon="@drawable/ic_settings_wireless">
            <intent
                android:action="android.intent.action.MAIN"
                android:targetPackage="com.android.settings"
                android:targetClass="com.android.settings.wifi.WifiSettings" />
        </com.android.settings.IconPreferenceScreen>

        <!-- Bluetooth settings -->
        <com.android.settings.IconPreferenceScreen
                android:key="bt_settings"
                android:title="@string/settings_title_bluetooth"
                settings:icon="@drawable/ic_settings_bluetooth2">
            <intent
                android:action="android.intent.action.MAIN"
                android:targetPackage="com.android.settings"
                android:targetClass="com.android.settings.bluetooth.BluetoothSettings" />
        </com.android.settings.IconPreferenceScreen>

        <!-- Mobile networks -->
        <com.android.settings.IconPreferenceScreen
                android:title="@string/settings_title_mobile"
                settings:icon="@drawable/ic_settings_mobilenetworks">
            <intent
                android:action="android.intent.action.MAIN"
                android:targetPackage="com.android.phone"
                android:targetClass="com.android.phone.Settings" />
        </com.android.settings.IconPreferenceScreen>

        <!-- More -->
        <com.android.settings.IconPreferenceScreen
                android:title="@string/settings_title_more"
                settings:icon="@drawable/ic_settings_more">
            <intent
                android:action="android.intent.action.MAIN"
                android:targetPackage="com.android.settings"
                android:targetClass="com.android.settings.WirelessSettings" />
        </com.android.settings.IconPreferenceScreen>

    <!-- DEVICE -->
    <PreferenceCategory
            android:title="@string/header_category_device" />

        <!-- Sound -->
        <com.android.settings.IconPreferenceScreen
                android:title="@string/settings_title_sounds"
                settings:icon="@drawable/ic_settings_sound">
            <intent
                android:action="android.intent.action.MAIN"
                android:targetPackage="com.android.settings"
                android:targetClass="com.android.settings.SoundSettings" />
        </com.android.settings.IconPreferenceScreen>

        <!-- Display -->
        <com.android.settings.IconPreferenceScreen
                android:title="@string/settings_title_display"
                settings:icon="@drawable/ic_settings_display">
            <intent
                android:action="android.intent.action.MAIN"
                android:targetPackage="com.android.settings"
                android:targetClass="com.android.settings.DisplaySettings" />
        </com.android.settings.IconPreferenceScreen>

        <!-- Lock screen -->
        <com.android.settings.IconPreferenceScreen
                android:title="@string/settings_title_lockscreen"
                settings:icon="@drawable/ic_settings_lockscreen">
            <intent android:action="android.intent.action.MAIN"
                    android:targetPackage="com.android.settings"
                    android:targetClass="com.android.settings.LockscreenSettings" />
        </com.android.settings.IconPreferenceScreen>

        <!-- Buttons -->
        <com.android.settings.IconPreferenceScreen
                android:title="@string/settings_title_buttons"
                settings:icon="@drawable/ic_settings_buttons">
            <intent android:action="android.intent.action.MAIN"
                    android:targetPackage="com.android.settings"
                    android:targetClass="com.android.settings.ButtonSettings" />
        </com.android.settings.IconPreferenceScreen>

        <!-- Battery -->
        <com.android.settings.IconPreferenceScreen
                android:title="@string/settings_title_battery"
                settings:icon="@drawable/ic_settings_battery">
            <intent android:action="android.intent.action.MAIN"
                    android:targetPackage="com.android.settings"
                    android:targetClass="com.android.settings.fuelgauge.PowerUsageSummary" />
        </com.android.settings.IconPreferenceScreen>

        <!-- Security -->
        <com.android.settings.IconPreferenceScreen
                android:title="@string/settings_title_security"
                settings:icon="@drawable/ic_settings_security">
            <intent
                android:action="android.intent.action.MAIN"
                android:targetPackage="com.android.settings"
                android:targetClass="com.android.settings.SecuritySettings" />
        </com.android.settings.IconPreferenceScreen>

        <!-- Interface -->
        <com.android.settings.IconPreferenceScreen
                android:key="dock_settings"
                android:title="@string/settings_title_interface"
                settings:icon="@drawable/ic_settings_interface">
            <intent
                android:action="android.intent.action.MAIN"
                android:targetPackage="com.android.settings"
                android:targetClass="com.android.settings.InterfaceSettings" />
        </com.android.settings.IconPreferenceScreen>

        <!-- Device Settings -->
        <com.android.settings.IconPreferenceScreen
                android:key="device_settings"
                android:title="@string/settings_title_device"
                settings:icon="@drawable/ic_settings_device">
            <intent
                android:action="com.cyanogenmod.action.LAUNCH_DEVICE_SETTINGS"/>
        </com.android.settings.IconPreferenceScreen>

    <!-- PERSONAL -->
    <PreferenceCategory
            android:title="@string/header_category_personal" />

        <!-- Launcher -->
        <com.android.settings.IconPreferenceScreen
                android:key="launcher_settings"
                android:title="@string/settings_title_launcher"
                settings:icon="@drawable/ic_settings_launcher">
            <intent
                android:action="android.intent.action.MAIN"
                android:targetPackage="com.android.launcher"
                android:targetClass="com.android.launcher.MyLauncherSettings" />
        </com.android.settings.IconPreferenceScreen>

        <!-- Profiles -->
        <com.android.settings.IconPreferenceScreen
                android:title="@string/settings_title_profiles"
                settings:icon="@drawable/ic_settings_profile">
            <intent
                android:action="android.intent.action.MAIN"
                android:targetPackage="com.android.settings"
                android:targetClass="com.android.settings.ProfileList" />
        </com.android.settings.IconPreferenceScreen>

        <!-- Status bar -->
        <com.android.settings.IconPreferenceScreen
                android:title="@string/settings_title_status_bar"
                settings:icon="@drawable/ic_settings_status_bar">
            <intent
                android:action="android.intent.action.MAIN"
                android:targetPackage="com.android.settings"
                android:targetClass="com.android.settings.StatusBarSettings" />
        </com.android.settings.IconPreferenceScreen>

        <!-- Notification drawer -->
        <com.android.settings.IconPreferenceScreen
                android:title="@string/settings_title_notification_drawer"
                settings:icon="@drawable/ic_settings_notification_drawer">
            <intent
                android:action="android.intent.action.MAIN"
                android:targetPackage="com.android.settings"
                android:targetClass="com.android.settings.NotificationDrawerSettings" />
        </com.android.settings.IconPreferenceScreen>

        <!-- Application Settings -->
        <com.android.settings.IconPreferenceScreen
                android:title="@string/settings_title_applications"
                settings:icon="@drawable/ic_settings_applications">
            <intent
                android:action="android.intent.action.MAIN"
                android:targetPackage="com.android.settings"
                android:targetClass="com.android.settings.ApplicationSettings" />
        </com.android.settings.IconPreferenceScreen>

        <!-- Data Sync. The settings activity will ensure this is resolved to an
             activity on the system image, otherwise it will remove this
             preference. -->
        <com.android.settings.IconPreferenceScreen
                android:key="sync_settings"
                android:title="@string/settings_title_accounts"
                settings:icon="@drawable/ic_settings_sync">
            <intent android:action="android.settings.SYNC_SETTINGS" />
        </com.android.settings.IconPreferenceScreen>

        <!-- Privacy -->
        <com.android.settings.IconPreferenceScreen
                android:title="@string/settings_title_privacy"
                settings:icon="@drawable/ic_settings_privacy">
            <intent
                android:action="android.intent.action.MAIN"
                android:targetPackage="com.android.settings"
                android:targetClass="com.android.settings.PrivacySettings" />
        </com.android.settings.IconPreferenceScreen>

        <!-- Location -->
        <com.android.settings.IconPreferenceScreen
                android:title="@string/settings_title_location"
                settings:icon="@drawable/ic_settings_location">
            <intent
                android:action="android.intent.action.MAIN"
                android:targetPackage="com.android.settings"
                android:targetClass="com.android.settings.LocationSettings" />
        </com.android.settings.IconPreferenceScreen>

    <!-- SYSTEM -->
    <PreferenceCategory
            android:title="@string/header_category_system" />

        <!-- Date & Time -->
        <com.android.settings.IconPreferenceScreen
                android:title="@string/settings_title_date_time"
                settings:icon="@drawable/ic_settings_date_time">
            <intent
                android:action="android.intent.action.MAIN"
                android:targetPackage="com.android.settings"
                android:targetClass="com.android.settings.DateTimeSettings" />
        </com.android.settings.IconPreferenceScreen>

        <!-- Accessibility feedback -->
        <com.android.settings.IconPreferenceScreen
                android:title="@string/settings_title_accessibility"
                settings:icon="@drawable/ic_settings_accessibility">
            <intent
                android:action="android.intent.action.MAIN"
                android:targetPackage="com.android.settings"
                android:targetClass="com.android.settings.AccessibilitySettings" />
        </com.android.settings.IconPreferenceScreen>

        <!-- Storage -->
        <com.android.settings.IconPreferenceScreen
                android:title="@string/settings_title_storage"
                settings:icon="@drawable/ic_settings_storage">
            <intent android:action="android.intent.action.MAIN"
                    android:targetPackage="com.android.settings"
                    android:targetClass="com.android.settings.deviceinfo.Memory" />
        </com.android.settings.IconPreferenceScreen>

        <!-- Language & input -->
        <com.android.settings.IconPreferenceScreen
                android:title="@string/settings_title_language"
                settings:icon="@drawable/ic_settings_language">
            <intent
                android:action="android.intent.action.MAIN"
                android:targetPackage="com.android.settings"
                android:targetClass="com.android.settings.LanguageSettings" />
        </com.android.settings.IconPreferenceScreen>

        <!-- Performance -->
        <com.android.settings.IconPreferenceScreen
                android:title="@string/settings_title_performance"
                settings:icon="@drawable/ic_settings_performance">
            <intent android:action="android.intent.action.MAIN"
                    android:targetPackage="com.android.settings"
                    android:targetClass="com.android.settings.PerformanceSettings" />
        </com.android.settings.IconPreferenceScreen>

        <!-- Developer options -->
        <com.android.settings.IconPreferenceScreen
                android:title="@string/settings_title_development"
                settings:icon="@drawable/ic_settings_development">
            <intent android:action="android.intent.action.MAIN"
                    android:targetPackage="com.android.settings"
                    android:targetClass="com.android.settings.DevelopmentSettings" />
        </com.android.settings.IconPreferenceScreen>

        <!-- About Device -->
        <com.android.settings.IconPreferenceScreen
                android:title="@string/settings_title_about"
                settings:icon="@drawable/ic_settings_about">
            <intent
                android:action="android.intent.action.MAIN"
                android:targetPackage="com.android.settings"
                android:targetClass="com.android.settings.DeviceInfoSettings" />
        </com.android.settings.IconPreferenceScreen>

</PreferenceScreen>

